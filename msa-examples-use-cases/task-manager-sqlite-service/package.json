{
  "name": "msa-task-manager-sqlite-service",
  "version": "0.1.0",
  "description": "Task Manager SQLite service example using @arifwidianto/msa-core and plugins",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "lint": "eslint src/**/*.ts",
    "clean": "rimraf dist src/**/*.db tasks.db",
    "check": "npm run build && ts-node src/check.ts",
    "db:init": "ts-node src/db-init.ts"
  },
  "dependencies": {
    "@arifwidianto/msa-core": "file:../../packages/core",
    "@arifwidianto/msa-plugin-http": "file:../../packages/plugin-http",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "typescript": "^5.4.5",
    "ts-node": "^10.9.2",
    "@types/node": "^20.12.12",
    "@types/sqlite3": "^3.1.11",
    "rimraf": "^5.0.5",
    "axios": "^1.6.0",
    "eslint": "^8.52.0",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1"
  }
}
